<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <FrameLayout style="@style/layoutmatchparent">
        <android.support.v4.view.ViewPager
            style="@style/layoutmatchparent"
            android:id="@+id/familyuser_dataNalysis_viewpter">
        </android.support.v4.view.ViewPager>
        <RelativeLayout style="@style/layoutWrapContent"
            android:layout_marginTop="325dp"
            android:layout_gravity="center_horizontal">
            <ImageView android:id="@+id/imagA"
                style="@style/layoutWrapContent"
                android:src="@drawable/imselect"/>
            <ImageView
                android:layout_toRightOf="@+id/imagA"
                android:id="@+id/imagB"
                android:layout_marginLeft="10dp"
                style="@style/layoutWrapContent"
                android:src="@drawable/imselect"/>
        </RelativeLayout>
    </FrameLayout>
</LinearLayout>